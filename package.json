{
  "name": "mypickle",
  "version": "1.0.0",
  "description": "[![js-standard-style](https://cdn.rawgit.com/standard/standard/master/badge.svg)](https://github.com/standard/standard)",
  "main": "index.js",
  "scripts": {
    "start": "cd server && npm run start",
    "build": "cd client && npm run build",
    "dev:start": "cd server && npm run dev:start",
    "dev:both": "concurrently --kill-others-on-fail \"cd client && npm run start\" \"npm run dev:start\" ",
    "test:client": "cd client && npm test",
    "client:init": "cd client && npm i",
    "server:init": "cd server && npm i",
    "both:init": "npm run client:init && npm run server:init",
    "coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techforbetter/myPickle.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/techforbetter/myPickle/issues"
  },
  "homepage": "https://github.com/techforbetter/myPickle#readme",
  "devDependencies": {
    "codecov": "^3.1.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.15.3"
  }
}
